{
    "$schema": "https://turborepo.org/schema.json",
    "tasks": {
      "dev": {
        "cache": false,
        "persistent": true
      },
      "build": {
        "outputs": ["dist/**", ".next/**", "build/**"],
        "dependsOn": ["^build"]
      },
      "lint": {
        "outputs": []
      },
      "test": {
        "outputs": ["coverage/**"],
        "dependsOn": ["^build"]
      },
      "test:e2e": {
        "outputs": []
      },
      "type-check": {
        "outputs": []
      }
    }
  }
  